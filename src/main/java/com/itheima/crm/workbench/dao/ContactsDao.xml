<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.crm.workbench.dao.ContactsDao">
    <insert id="save">
        INSERT INTO `tbl_contacts`(
            `id`
            ,`owner`
            ,`source`
            ,`customerId`
            ,`fullname`
            ,`appellation`
            ,`email`
            ,`mphone`
            ,`job`
            ,`birth`
            ,`createBy`
            ,`createTime`
            ,`description`
            ,`contactSummary`
            ,`nextContactTime`
            ,`address`
        ) VALUES(
            #{id}
            ,#{owner}
            ,#{source}
            ,#{customerId}
            ,#{fullname}
            ,#{appellation}
            ,#{email}
            ,#{mphone}
            ,#{job}
            ,#{birth}
            ,#{createBy}
            ,#{createTime}
            ,#{description}
            ,#{contactSummary}
            ,#{nextContactTime}
            ,#{address}
        )
    </insert>
</mapper>